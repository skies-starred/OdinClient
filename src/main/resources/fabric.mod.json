{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "OdinClient",
  "description": "A Hypixel Skyblock odin addon for cheats",
  "authors": ["Starred"],
  "contact": {},
  "license": "BSD-3-Clause",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "starred.skies.odin.OdinClient"
      }
    ]
  },
  "mixins": [
    "odinClient.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.17",
    "fabric-api": "*",
    "fabric-language-kotlin": "*",
    "minecraft": "${minecraft}",
    "odin": "*"
  }
}
